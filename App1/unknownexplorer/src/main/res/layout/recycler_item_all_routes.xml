<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/info_about_route_element"
    android:layout_width="match_parent"
    android:layout_height="230dp"
    android:background="#90CCCCCC"
    android:orientation="vertical"
    android:padding="5dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FDFDFD">

        <View
            android:id="@+id/divider"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginBottom="32dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_description_router_all_routes"
            app:layout_constraintVertical_bias="0.52" />

        <TextView
            android:id="@+id/text_title_router_all_routes"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:text="TextView"
            android:textAlignment="viewStart"
            android:textColor="#898989"
            android:textSize="22sp"
            android:textStyle="italic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_conversion_absoluteHeight="136dp"
            tools:layout_conversion_absoluteWidth="0dp" />

        <TextView
            android:id="@+id/text_description_router_all_routes"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="4dp"
            android:background="#004CAF50"
            android:fadeScrollbars="true"
            android:text="TextView"
            android:textAlignment="viewStart"
            android:textColor="#898989"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/divider"
            app:layout_constraintEnd_toEndOf="@+id/text_title_router_all_routes"
            app:layout_constraintStart_toStartOf="@+id/text_title_router_all_routes"
            app:layout_constraintTop_toBottomOf="@+id/text_title_router_all_routes"
            tools:layout_conversion_absoluteHeight="136dp"
            tools:layout_conversion_absoluteWidth="0dp" />

        <TextView
            android:id="@+id/text_interest_router_my_routes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="4dp"
            android:shadowColor="#898989"
            android:text="Interes"
            android:textColor="#898989"
            app:layout_constraintEnd_toStartOf="@+id/textView5"
            app:layout_constraintStart_toStartOf="@+id/text_description_router_all_routes"
            app:layout_constraintTop_toBottomOf="@+id/divider" />

        <TextView
            android:id="@+id/text_type_router_all_routes"
            style="Lab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="4dp"
            android:shadowColor="#898989"
            android:text="type"
            android:textColor="#898989"
            app:layout_constraintEnd_toStartOf="@+id/textView7"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toTopOf="@+id/textView5" />

        <TextView
            android:id="@+id/text_time_router_all_routes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="4dp"
            android:shadowColor="#898989"
            android:text="time"
            android:textColor="#898989"
            app:layout_constraintEnd_toStartOf="@+id/textView8"
            app:layout_constraintStart_toEndOf="@+id/textView7"
            app:layout_constraintTop_toTopOf="@+id/textView7" />

        <TextView
            android:id="@+id/text_rating_router_all_routes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="8dp"
            android:shadowColor="#898989"
            android:text="rating"
            android:textColor="#898989"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintTop_toTopOf="@+id/textView8" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:background="#7495A791"
            android:text="Title "
            android:textColor="#898989"
            app:layout_constraintStart_toStartOf="@+id/text_title_router_all_routes"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="4dp"
            android:layout_height="19dp"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="4dp"
            android:text="•"
            android:textColor="#898989"
            app:layout_constraintEnd_toStartOf="@+id/text_type_router_all_routes"
            app:layout_constraintStart_toEndOf="@+id/text_interest_router_my_routes"
            app:layout_constraintTop_toTopOf="@+id/text_interest_router_my_routes" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="4dp"
            android:text="•"
            android:textColor="#898989"
            app:layout_constraintEnd_toStartOf="@+id/text_time_router_all_routes"
            app:layout_constraintStart_toEndOf="@+id/text_type_router_all_routes"
            app:layout_constraintTop_toTopOf="@+id/text_type_router_all_routes" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="4dp"
            android:text="•"
            android:textColor="#898989"
            app:layout_constraintEnd_toStartOf="@+id/text_rating_router_all_routes"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/text_time_router_all_routes"
            app:layout_constraintTop_toTopOf="@+id/text_time_router_all_routes" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</LinearLayout>
